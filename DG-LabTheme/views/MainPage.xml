<?xml version="1.0" encoding="UTF-8"?>

<odoo>
	<template id="DGLab" name="DGLab">
		<t t-call="website.layout">

			<body>

				<!--MENU BAR PALMERAS-->
			  <div class="MENUBAR">
			    <ul>
			      <li>Login</li>
						<li>Nosotros / Contacto</li>
						<li>Digital Store</li>
						<li>Galería</li>
						<li>Construcción / Equipamiento</li>
					</ul>
				</div>



				<!--MAIN SPLASH START-->
				<div class="MAIN-SPLASH">
					<div class="MAIN-WAVE-TOP">
						<img src="../static/src/images/Main-Curv-Bottom-Menu.png" width="100%" alt="Wave Graphic"/>
					</div>

					<div class="MAIN-IMAGE">
						<img src="../static/src/images/Costa-Palmeras-Splash-Image.png" width="100%" alt="Costa Palmeras Splash Image"/>
					</div>

					<div class="MAIN-WAVE-YELLOW">
						<img src="../static/src/images/Main-Wave-Yellow.png" width="100%" alt="Costa Palmeras Wave Yellow"/>
					</div>
			    <div class="MAIN-TYPOGRAPHY">
			      FELIZ PISCINA
			    </div>

			    <div class="MAIN-WAVE-YELLOW">
			    </div>

					<!--MAIN SPLASH END-->
				</div>


				<!--HYPE ANIMATION-->
				<div style ="margin:auto;width:600px">
					<!-- copy these lines to your document: -->
					<div id="noend_hype_container" style="position:relative;overflow:hidden;width:600px;height:400px;">
						<script type="text/javascript" charset="utf-8" src="/DG-LabTheme/static/src/js/NoEnd.hyperesources/noend_hype_generated_script.js?67420">
						</script>
					</div>
				</div>

			</body>
		</t>
	</template>

	<delete model="website.page" search="[('id','=',ref('website.homepage_page'))]"/>
	<record id="homepage_page" model="website.page">
		<field name="website_published">True</field>
		<field name="url">/</field>
		<field name="view_id" ref="DGLab"/>
	</record>
</odoo>
