<?xml version="1.0" encoding="UTF-8"?>

<odoo>
	<template id="DGLab" name="DGLab">
		<t t-call="website.layout">

			<body>

				<!--MENU BAR PALMERAS-->
			  <div class="MENUBAR">
			    <ul>
			      <li>Login</li>
						<li>Nosotros / Contacto</li>
						<li>Digital Store</li>
						<li>Galería</li>
						<li>Construcción / Equipamiento</li>
					</ul>
				</div>



				<!--MAIN SPLASH START-->
				<div class="MAIN-SPLASH">
					<div class="MAIN-WAVE-TOP">
						<img src="/DG-LabTheme/static/src/images/Main-Curv-Bottom-Menu.png" width="100%" alt="Wave Graphic"/>
					</div>

					<div class="MAIN-IMAGE">
						<img src="/DG-LabTheme/static/src/images/Costa-Palmeras-Splash-Image.png" width="100%" alt="Costa Palmeras Splash Image"/>
					</div>

					<div class="MAIN-WAVE-YELLOW">
						<img src="/DG-LabTheme/static/src/images/Main-Wave-Yellow.png" width="100%" alt="Costa Palmeras Wave Yellow"/>
					</div>
			    <div class="MAIN-TYPOGRAPHY">
			      FELIZ PISCINA
			    </div>

			    <div class="MAIN-WAVE-YELLOW">
			    </div>

					<!--MAIN SPLASH END-->
				</div>


				<div id="serviciospiletas_hype_container" style="margin:auto;position:relative;width:970px;height:500px;overflow:hidden;">
					<script type="text/javascript" charset="utf-8" src="/DG-LabTheme/static/src/js/Servicios-Piletas.hyperesources/serviciospiletas_hype_generated_script.js?42677"></script>
				</div>



			</body>
		</t>
	</template>

	<delete model="website.page" search="[('id','=',ref('website.homepage_page'))]"/>
	<record id="homepage_page" model="website.page">
		<field name="website_published">True</field>
		<field name="url">/</field>
		<field name="view_id" ref="DGLab"/>
	</record>
</odoo>
